<script type="text/javascript">
    var d = new Date();
    var h = d.getHours();
    var ht = '9:00-11:00';
    if (h < 11) {
        ht = "9:00-11:00";
    } else if (h < 14) {
        ht = "11:01-14:00";
    } else if (h < 17) {
        ht = "14:01-16:30";
    }
    if (h > 16) {
        d.setDate(d.getDate() + 1);
    }

    var day = d.getFullYear() + "年" + (d.getMonth() + 1) + "月" + d.getDate() + "日";


    document.getElementById("yj_time").innerHTML = ht;
    document.getElementById("yj_date").innerHTML = day;

    var n = Math.random() * 150;
    n = (n + 50).toFixed(0);
    document.getElementById("yj_pnum").innerHTML = n;
    
