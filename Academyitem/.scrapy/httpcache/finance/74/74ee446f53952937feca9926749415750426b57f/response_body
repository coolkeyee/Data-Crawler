function getPage( obj ) {
    var url = obj.attr('href')+'?'+Math.random();
    if( url == 'javascript:void(0);' ) {
        return false;
    }
    // $.ajaxSetup({cache:false});
    $.get( url , {} , function( data ) {
      $('#datalist').html( data );
      creatBorder();
      $('html,body').animate({ scrollTop : parseInt( $('#datalist').offset().top - 230 )} , 500 );
    })
}

    
function creatBorder(){
    $('.s_border').each(function(index, element){
        if($(this).length < 1) return;
        var dom = '<div id="line'+index+'" style="position: absolute; left: 0; right: 0; top: 0; bottom: 0;"></div>';
        $(this).append(dom);
        cline('#line'+index);
    });
}

function cline(obj){
    var $this = $(obj);
    var par = $this.closest('.s_border');
    var paper = Raphael($this[0]);
    var w = $this.width();
    var h = $this.height();
    var lw = w - 9;
    var lh = h - 9;
    
    var sc = par.data('sc') || '#a6937f';
    var ec = par.data('ec') || '#6a312c';
    
    var line1, line2;
    
    if(w > h){
        line1 = paper.path("M5, 9h3, v4, h-6, v-8, h3, v-4, h" + lw + ", v4, h3, v8, h-6, v-4, h3");
        line2 = paper.path("M5, " + (h - 10) + "h3, v-4, h-6, v8, h3, v4, h" + lw + ", v-4, h3, v-8, h-6, v4, h3");
    }else{
        line1 = paper.path("M9, 5v3, h4, v-6, h-8, v3, h-4, v" + lh + ", h4, v3, h8, v-6, h-4, v3");
        line2 = paper.path("M" + (w - 10) + ", 5v3, h-4, v-6, h8, v3, h4, v" + lh + ", h-4, v3, h-8, v-6, h4, v3");
    }
    
    if(par.hasClass('cur')){
        line1.attr({stroke: ec});
        line2.attr({stroke: ec});
    }else{
        line1.attr({stroke: sc});
        line2.attr({stroke: sc});
    }
    
    line1.attr({'stroke-width': 2});
    line2.attr({'stroke-width': 2});
    
    par.on('click', function(){
        $(this).siblings().find('path').attr({stroke: sc});
    });

    par.on('mouseenter', function(){
        line1.attr({stroke: ec});
        line2.attr({stroke: ec});
    });
}
</script>
