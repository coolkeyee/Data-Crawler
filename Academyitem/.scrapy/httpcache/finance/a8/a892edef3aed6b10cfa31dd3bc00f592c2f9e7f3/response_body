<script>
//检索框的部分
function searchCK(){
var reg= /\<|\>|%|\&|\+|\-|\||\;|\$|\@|\'|\"|\(|\)|\,/g;

	 if(reg.test(document.getElementById("content").value)){
		alert('您输入的信息包含非法字符!');
		return false;
}

    if (document.getElementById("content").value =="")
      {
        alert("请输入检索内容!");
        return false;
      }else{
		  var url = "http://www.19371213.com.cn/jsjg/";
		  //alert(url);
		 window.location.href = url+"?word="+document.getElementById("content").value;
	  }
}
</script>


