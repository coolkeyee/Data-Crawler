               jQuery("#en").click(function(){
            var urlArray=cur_url.split("/");
            var afterUrl ="";
           
            for(var i=3;i<urlArray.length;i++){
                afterUrl+="/"+urlArray[i];
            }
            var goto = urlArray[0] + "/en" + afterUrl;
            window.location.href=goto;
        })        
             
               jQuery("#ja").click(function(){
            var urlArray=cur_url.split("/");
            var afterUrl ="";
          
            for(var i=3;i<urlArray.length;i++){
                afterUrl+="/"+urlArray[i];
            }
            var goto = urlArray[0] + "/ja" + afterUrl;
            window.location.href=goto;
        })        
