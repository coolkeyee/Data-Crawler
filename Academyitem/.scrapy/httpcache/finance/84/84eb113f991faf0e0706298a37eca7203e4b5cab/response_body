    /*选项卡切换*/
    function clickTab(sender) {
        $(sender).addClass('selected').siblings().removeClass('selected');
        var index = $('#tab ul li').index(sender);
        $('div.tab_box > div').eq(index).show().siblings().hide();
        //$('.down_arrow').hide();
        //$('.down_arrow').eq(index).show();
    }

    /*查看产品详细*/
    function viewdetail() {
        shopcontent.innerHTML = productdetail.innerHTML;
    }

    /*返回商品列表*/
    function backList() {
        shopcontent.innerHTML = productlist.innerHTML;
    }

    $(document).ready(function () {
        var $tab_li = $('#tab ul li');
        $tab_li.click(function () {
            clickTab(this);
        });
    });
</script>

<script>
	$(function(){
		var end1 = $(".youshangjiao a:last").text();
		var end2 = $(".youshangjiao a:nth-last-child(2)").text();
		console.log(end1);console.log(end2);
		console.log("二级栏目1"+$(".tab_menu a").text());
		// 二级导航
		$(".tab_menu a").each(function(){
			console.log("二级栏目2"+$(".tab_menu a").text());
			if($(this).text()==end1){
				console.log("二级导航11"+$(this).text());
				$(this).parent("li").addClass("selected");
			}else{
				console.log("二级导航12"+$(this).text());
				if($(this).text()==end2){
					console.log("二级导航21"+$(this).text());
					$(this).parent("li").addClass("selected");
					// 三级导航
					$(".innertab_menu a").each(function(){
						if($(this).text()==end1){
							console.log("三级导航"+$(this).text());
							$(this).parent("li").addClass("selected innerselected");
						}
					});
				}else{
					console.log("二级导航22"+$(this).text());
				}
			}
		});
	})
</script>
