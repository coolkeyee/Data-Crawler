<script>
    $(function () {
//        $(".sub_menu_item").on("click",function(){
//            window.location.href = $(this).attr("href");
//        });

        $(".foot_link").on("click",function () {
            window.location.href = $(this).attr("href");
        });
        $("#search_submit").on("click",function () {
            var input = $("#global_search").val();
            if(!input){
                return;
            }
            window.location.href="website/pages/search.php?search="+input;
        });
    });
</script>


