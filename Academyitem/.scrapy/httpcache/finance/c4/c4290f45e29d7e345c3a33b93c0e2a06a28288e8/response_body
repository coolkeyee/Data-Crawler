	<script>
			function toSearch(urlStr,siteid) {
	/*  去掉字符前后的空格正则表达式*/
			var keyword=document.getElementById("keyword1").value;
			var key=keyword.replace(/\s+/g,"")
			var keyWord1 = encodeURI(key);
			keyWord1 = encodeURI(keyWord1);
			window.location = urlStr + "&keyword=" + keyWord1+"&siteid="+siteid;
		}
