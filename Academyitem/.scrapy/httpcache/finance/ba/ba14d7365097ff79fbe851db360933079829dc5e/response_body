	$(function(){
		$('.alerts .con1 .cimgbox .inner').on('init', function(e, s){
			$('.alerts .con1 .cimgbox .slick-paging').text( "0" + (s.currentSlide + 1) );
		});
			
		$('.alerts .con1 .cimgbox .inner').on('afterChange', function(e, s, c){
			$('.alerts .con1 .cimgbox .slick-paging').text( "0" + (c + 1) );
		});
		
		$('.alerts .con1 .cimgbox .inner').slick({
			asNavFor: '.alerts .con1 .rbox',
			dots: true,
			arrows: false,
		});
		
		$('.alerts .con1 .rbox').slick({
			asNavFor: '.alerts .con1 .cimgbox .inner',
			fade: true,
			arrows: false,
			dots: false,
		});
		
		$('.alerts .con2').xRoll({
			target: '.li',
			stagger: 0.4,
			trans:{
				y: '-10%'
			}
		});
		
	})
</script>
