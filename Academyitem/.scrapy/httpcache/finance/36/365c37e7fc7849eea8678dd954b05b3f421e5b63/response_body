function showIntro(botId){
	$(".topContent").hide();
	$("#"+botId).show();
}
function preOne(){
	var id=$(".topNews:visible").attr("id");
	var intId = parseInt((id.substring(3, id.length)));
	intId--;
	if(intId<0){ //如果最小
		intId=4;
	}
	id="top"+intId;
	$(".topNews").hide();
	$("#"+id).show();
}
function nextOne(){
	var id=$(".topNews:visible").attr("id");
	var width=$(".topNews:visible").width();
	var height=$(".topNews:visible").height();
	var intId = parseInt((id.substring(3, id.length)));
	intId++;
	if(intId>4){ //如果最小
		intId=0;
	}
	id="top"+intId;
	$(".topNews").hide();
	var topNews=$("#"+id).width();
	$("#"+id).fadeIn("slow");
}
</script></head>


