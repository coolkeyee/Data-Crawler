        function Search()
        {
            
            var txt = document.all.search_text;
            var down = document.all.search_li;
            
                        
            if (trim(txt.value) == "")
            {
                alert('请输入查询关键字!');
                return;
            }
            
            var typ = "title";
            
            switch(down.selectedIndex)
            {
                case 0: typ ="Title";break;
                case 1: typ ="Detail";break;
                case 2: typ ="Writer";break; 
                case 3: typ ="KeyWords";break; 
                default: break;               
            }
            //ID=133其中133为栏目中任意一个存在的栏目id
            document.location="http://www.xabwy.com/SitePage.aspx?ID=133&VID=-1&Url=SiteSearch.html&SearchKey="+txt.value+"&SearchType="+typ;
            
        }
