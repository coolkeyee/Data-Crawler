													if(niandai!=undefined&&niandai!=""){
													   //$("#niandai").val(niandai);
													   selectNumber('niandai',niandai)
													}
													if(zhidi!=undefined&&zhidi!=""){
													   //$("#zhidi").val(zhidi);
													   selectNumber('zhidi',zhidi)
													}
													if(pinlei!=undefined&&pinlei!=""){
												
													   //$("#pinlei").val(pinlei);
													   selectNumber('pinlei',pinlei)
													}
													if(SearchWord!=undefined&&SearchWord!=""){
													$("#word").val(SearchWord);
													}
												   
												   
												   
												   
													
													searchStart('0');
												   }

							   function selectNumber(id,value){
									var num = value;   //获取input中输入的数字
									var numbers = $("#"+id).find("option"); //获取select下拉框的所有值
									for (var j = 1; j < numbers.length; j++) {
										if ($(numbers[j]).val() == num) {
											$(numbers[j]).attr("selected", "selected");
											$("#"+id).next('.cz-dropbox').find('span.cz-dropbox-caption').text($(numbers[j]).text())
											$("#"+id).next('.cz-dropbox').find('.cz-dropbox-input').val(num)
										}
									 } 
								}

					

				
						
	 
			});


	   
