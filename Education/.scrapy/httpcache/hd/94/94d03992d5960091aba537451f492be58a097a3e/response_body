	<script type="text/javascript">



	function getParameter(param){  
									var query = window.location.search; 
									var iLen = param.length;
									var iStart = query.indexOf(param); 
									if (iStart == -1) 
										return "";  
									iStart += iLen + 1;  
									var iEnd = query.indexOf("&", iStart);  
									if (iEnd == -1)  
										return query.substring(iStart);  
									return query.substring(iStart, iEnd);
								}





					//控制检索范围的字段
					var chnlslimit = "73";
					//分类检索的固定字段
					var chnls="73";
					var SearchScope ="DOCTITLE";
					var TimeScope = null;
					var SortOrder = "+RELEVANCE";
					var SearchWord =null;
					//开始查询的位置 以0为开始
					var PageNum =1;
					var PageSize =20;
					//文章数量
					var recordCount=0;
					//藏品分类，0为所有
					var cutsize=200;
					//检索花费时间
					var spendTime=1;
					var classification="";
					var niandai='';
					var zhidi='';
					var pinlei='';
					var notchnls=null
					var wwfl=getParameter("wwfl");//文物分类
					var xsfl="";//下属分类
					
				function searchStart(value) {
				  var PageNum =1;
							  SearchWord =null;
					var values = value.split('=');
				if("PageNum"==values[0]){
						PageNum=values[1];
					}else if("xsfl"==values[0]){
					xsfl=values[1];
					}else{
					}
				
				  
					//分类chnls需要的栏目id
					
					if($("#niandai").next('.cz-dropbox').find('.cz-dropbox-input').attr('value')!="" && $("#niandai").next('.cz-dropbox').find('.cz-dropbox-input').attr('value')){
					classification+=$("#niandai").next('.cz-dropbox').find('.cz-dropbox-input').attr('value')+",";
					}
					if($("#pinlei").next('.cz-dropbox').find('.cz-dropbox-input').attr('value')!="" && $("#pinlei").next('.cz-dropbox').find('.cz-dropbox-input').attr('value')){
					classification+=$("#pinlei").next('.cz-dropbox').find('.cz-dropbox-input').attr('value')+",";
					}
					if($("#zhidi").next('.cz-dropbox').find('.cz-dropbox-input').attr('value')!="" && $("#zhidi").next('.cz-dropbox').find('.cz-dropbox-input').attr('value')){
					classification+=$("#zhidi").next('.cz-dropbox').find('.cz-dropbox-input').attr('value')+",";
					}
					classification+=wwfl;
					if(xsfl!=""){
					classification+=","+xsfl;
					}
					SearchWord=$("#word").val()
									
					 if(classification == ''){
						classification = '0'
					 }    

					if(SearchWord==null||SearchWord==""||SearchWord=="请输入藏品关键词"){
					SearchWord='';
					}
						$.ajax({
						url: "/SearchAll",
						data: {
							//控制检索范围的字段
							"chnlslimit" :chnlslimit,
							//检索范围
							"SearchScope": SearchScope,
							//时间范围
							"TimeScope": TimeScope,
							//排列方式
							"SortOrder": SortOrder,
							//检索词
							"SearchWord": SearchWord,
							//显示多少条
							"PageSize": PageSize,
							//第几页
							"PageNum":PageNum,
							//分类检索固定字段
							"chnls":chnls,
							"classification":classification,
							 "cutsize":cutsize,
							 "notchnls":notchnls
						},
						success: function(value) {
							
							
							var t="";
							
							var j= eval('(' + value + ')');
							
							
							var ulHtml="";
							for(var i = 0; i < j.length; i++) {
							
								
								ulHtml+='<li><dl>';
								
								ulHtml+='<dt><a  target="_blank"   href="'+j[i]["dOCPUBURL"]+'" title="'+j[i]['dOCTITLE']+'"><img src="'+j[i]["slt"]+'" alt="" /></a></dt><dd><a  target="_blank"   href="'+j[i]["dOCPUBURL"]+'" title="'+j[i]['dOCTITLE']+'">';
							   
							   
								if(j[i]["nd"]==undefined||j[i]["nd"]==""){
								 ulHtml+='<span >&nbsp;</span>';
								 // console.log("执行了");
								}else{
                                                                 var reg = new RegExp(",","g");
								 ulHtml+='<span >'+j[i]["nd"].replace(reg,"·")+'</span>';
								}
								
								 // console.log(j[i]["nd"]);
							
								if(j[i]["dOCTITLE"].length>15){
								j[i]["dOCTITLE"]=j[i]["dOCTITLE"].substr(0,14)+"...";
								ulHtml+='<p>'+j[i]["dOCTITLE"]+'</p></a></dd></dl></a></li>';
								}else{
								ulHtml+='<p>'+j[i]["dOCTITLE"]+'</p></a></dd></dl></li>';
								}
								
								
							}
						
							$("#ulHtml").html(ulHtml);
							
							classification="";
													SearchWord =null;
						},
						error: function() {
							alert("error");

						}
						});
					
					$.ajax({
						url: "/Getsum",
						data: {
							//分类chnls需要的栏目id
							"chnlslimit":chnlslimit,
							//检索范围
							"SearchScope":SearchScope,
							//时间范围
							"TimeScope":TimeScope,
							//排列方式
							"SortOrder":SortOrder,
							//检索词
							"SearchWord":SearchWord,
							//显示多少条
							"PageSize":PageSize,
							//第几页
							"PageNum":PageNum,
							//需要检索的栏目id
							"chnls":chnls,
							"classification":classification,
													"cutsize":cutsize
						},
						success: function(value) {
							var j= eval('(' + value + ')');
							var a=j[0]["sum"]+":用时"+(j[1]["spendTime"]/1000)+"秒";
							
							recordCount=j[0]["sum"];
							spendTime=(j[1]["spendTime"]/1000);
							
							//$("#spendTimeAndSum").html("找到约"+recordCount+"条结果（用时"+spendTime+"秒）");
							
							var pageString =showpage(PageNum,recordCount,PageSize);
							$("#showpage").html(pageString);
							
						
						},
						error: function() {
							alert("error");

						}
					});
					
					

					
					

				}
			</script><script>
			   function keyup_submit(e){ 
				   var evt = window.event || e; 
					if (evt.keyCode == 13){
					 //回车事件
					 searchStart('0')
				   }
				 }
		  </script></head>







	
			 



