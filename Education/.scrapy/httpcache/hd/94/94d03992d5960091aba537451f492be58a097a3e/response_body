$(function(){
    var windowwidth=$(window).width();
      if(windowwidth<1200){
       $(".header,.footer").css("width","1200px");
      }else{
       $(".header,.footer").css("width","100%");
      }
    $(window).resize(function(){
     var windowwidth=$(window).width();
      if(windowwidth<1200){
       $(".header,.footer").css("width","1200px");
      }else{
       $(".header,.footer").css("width","100%");
      }
      
    });

